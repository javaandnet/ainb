    "jest-html-reporters": "^3.1.7",
    "lamejs": "^1.2.1",
    "npm-check-updates": "^16.14.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
